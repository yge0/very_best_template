<div class="page-header">
  <h3>Bookmark #<%= @bookmark.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Dish</dt>
      <dd><%= @bookmark.dish_id %></dd>

      <dt>Venue</dt>
      <dd><%= @bookmark.venue_id %></dd>

      <dt>User</dt>
      <dd><%= @bookmark.user_id %></dd>

      <dt>Notes</dt>
      <dd><%= @bookmark.notes %></dd>

      <dt>Image</dt>
      <dd><img src="<%= @bookmark.image %>" class="img-responsive"></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookmarks" class="btn btn-primary">
        Back
      </a>
      <a href="/bookmarks/<%= @bookmark.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_bookmark/<%= @bookmark.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
